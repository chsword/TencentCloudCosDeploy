{
  "manifestVersion": 1,
  "id": "chsword-tencent-cloud",
  "name": "Tencent Release Tools",
  "version": "1.0.74",
  "publisher": "chsword",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Deploy to Tencent Cloud Cos.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "logo.png"
  },
  "files": [
    { "path": "cosDeploy/node_modules" },
    { "path": "cosDeploy/index.js" },
    { "path": "cosDeploy/task.json" },
    { "path": "cosDeploy/src" },
    {
      "path": "logo.png",
      "addressable": true,
      "packagePath": "/cosDeploy/icon.png"
    }
  ],
  "contributions": [
    {
      "id": "ChswordTencentCloudCosDeploy",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "cosDeploy"
      }
    }
  ]
}
